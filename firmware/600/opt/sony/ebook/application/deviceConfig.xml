<deviceConfig xmlns="http://www.sony.com/xmlns/product/prs/device/1">
	<pm pmPath="/dev/npm"/>
	<nand/>
	<register/>
	<rtc rtcPath="/dev/rtc0" timeZonePath="/opt0/preferences/tz" clockPath="/opt0/preferences/clock">
		<initial year="2009" month="1" day="1" hour="0" minute="0" second="0"/>
		<autoStandby time="3600"/>
		<autoShutdown time="432000" batteryLevel="8" lowCaseTime="3600"/>
	</rtc>
	<log devicePath="/dev/ebklog" bufferSize="131072"/>
	<util orgFolder="/Data/tmp/database/" dstFolder="/Data/" checkedPath="/Data/tmp/checked">
		<preload file="/Data/tmp/preload.lst" prefix="/"/>
		<skip>
			<path value="/opt0/preferences/tz"/>
		</skip>
	</util>
	<usb/>
	<subcpu>
		<system exitCodeFile="/tmp/exitcode"/>
		<key/>
		<touchpanel width="600" height="800" sampling="30" breakThreshold="1" hysteresis="3" maxFilter="800" junk="1">
			<default>
				<calibrationPoint>
					<raw x="749" y="3530"/>
					<screen x="100" y="100"/>
				</calibrationPoint>
				<calibrationPoint>
					<raw x="3300" y="577"/>
					<screen x="500" y="700"/>
				</calibrationPoint>
			</default>
			<activeArea x="3" y="3" width="594" height="794"/>
		</touchpanel>
		<led/>
		<battery>
			<level min="0" max="31" logicalMin="0" logicalMax="100">
				<lessOrEqual logical="74" value="17"/>
				<lessOrEqual logical="50" value="13"/>
				<lessOrEqual logical="25" value="11"/>
				<lessOrEqual logical="1" value="9"/>
				<lessOrEqual logical="0" value="5"/>
			</level>
		</battery>
	</subcpu>
	<framebuffer width="600" height="800" depth="8" orientation="1" devicePath="/dev/fb0" checkPath="/proc/e-ink"/>
	<audio>
		<level min="0" max="50" logicalMin="0" logicalMax="256">
			<lessOrEqual logical="255" value="45"/>
			<lessOrEqual logical="242" value="43"/>
			<lessOrEqual logical="229" value="41"/>
			<lessOrEqual logical="216" value="39"/>
			<lessOrEqual logical="203" value="37"/>
			<lessOrEqual logical="191" value="35"/>
			<lessOrEqual logical="178" value="33"/>
			<lessOrEqual logical="165" value="31"/>
			<lessOrEqual logical="152" value="29"/>
			<lessOrEqual logical="139" value="27"/>
			<lessOrEqual logical="127" value="25"/>
			<lessOrEqual logical="114" value="23"/>
			<lessOrEqual logical="101" value="21"/>
			<lessOrEqual logical="88" value="19"/>
			<lessOrEqual logical="75" value="17"/>
			<lessOrEqual logical="64" value="15"/>
			<lessOrEqual logical="50" value="13"/>
			<lessOrEqual logical="37" value="10"/>
			<lessOrEqual logical="24" value="4"/>
			<lessOrEqual logical="11" value="0"/>
		</level>
	</audio>
	<memorycard/>
	<wm8350>
		<light>
			<step power="false" brightness="0"/>
			<step power="true" brightness="45"/>
			<step power="true" brightness="49"/>
		</light>
	</wm8350>
	<application/>
</deviceConfig>

