		<code>
			<function id="getKind">
				// now I set a value of about's kind.(temporary)
				return 25;
			</function>
			<function id="getFirstChild">
				return 0
			</function>
			<function id="getTitle">
				return "Test Mode"
			</function>
			<function id="dataChanged">
				this.pageChanged()
			</function>
			<function id="doRoot">
				this.doCenter()
			</function>
			<function id="doOption">
				this.doDown()
			</function>
			<function id="doSize">
				this.doUp()
			</function>
			<function id="doMenu">
				var model = kbook.model
				kbook.autoRunRoot.exitIf(model)
				model.container.clearModal()
			</function>
			<function id="variableChanged" params="id, value"><![CDATA[
				var count = this.countRecords();
				for (var i = 0; i < count; i++) {
					var record = this.getRecord(i)
					var variable = this.getValue(record, "variable")
					if (variable) {
						var value = this.getVariable(variable)
						record.comment = value
					}
					if (id == variable)
						this.dataChanged()
					if (id == "MENU_FOCUS")
						this.container.MENU.focus(true)
				}
			]]></function>
			<function id="openRecord" params="index"><![CDATA[
				var record = this.getRecord(index)
				var file = this.getValue(record, "file")
				if (file) {
					this.container.bubble("startTest", this.mergeURI("tests/" + file))
					return
				}
				var func = this.getValue(record, "func")
				this.container.bubble(func)
			]]></function>
			<function id="pageChanged">
				var c = this.countPages();
				var i = this.getPage() + 1;
				if (!c) c++;
				this.setVariable("MENU_INDEX_COUNT", i + " of " + c);
			</function>
		</code>
