is);		var state = this.getVariable("STATE");		var target;		if (this.state != state) {			this.focus(true)			this.AUTORUN_GROUP.show(false)			switch(state) {			case "AUTORUN":				this.loadScript();				this.AUTORUN_GROUP.show(true);				target = this.AUTORUN_GROUP.AUTORUN;				break;		}			this.state = state;			target.init();			 if (target.isEnabled()){	target.focus(true);	};		}	</function></code><group id="AUTORUN_GROUP" left="0" right="0" top="0" bottom="0" visible="false">	<panel skin="MahJongSkin" left="0" right="0" top="0" bottom="0"/> 		<sprite skin="hard-reset" left="0" width="70" top="0" height="70" active="false"/>		<kbookLabel skin="title" left="22" right="0" top="692" height="90" align="left" text="M"/>		<kbookLabel skin="versText" left="70" right="0" top="719" height="50" align="left" text="ahJonG 0.94"/>		<group id="AUTORUN" width="600" top="70" bottom="30">			<parts>            				<boolean key="0x30" do="doNext"/>				<boolean key="0x31" do="doPrevious"/>				<boolean key="0x30-hold" do="doFirst"/>				<boolean key="0x31-hold" do="doLast"/>			</parts>						<code>				<function id="doDigit" params="part">					<![CDATA[	this['goRow'](part.key)	]]>						</function>				<function id="doMenu">	this.doMenuF();</function>				<function id="doCenter">this.doCenterF();</function>				<function id="doMark">	this.loadGame();</function>				<function id="doNext">	this.moveCursor("right");</function>				<function id="doPrevious">this.moveCursor("left");</function>				<function id="doLast">	this.moveCursor("right");</function>				<function id="doFirst">	this.moveCursor("left");</function>				<function id="doLeft">	this.moveCursor("left");</function>				<function id="doRight">	this.moveCursor("right");</function>				<function id="doUp">	this.moveCursor("up");</function>				<function id="doDown">	this.moveCursor("down");</function>				<function id="doSize">	this.undoDel(this.BAK1, this.BAK2);</function>			</code>		<sprite skin='hel